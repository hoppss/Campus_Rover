<launch>
  <!-- test with -->
  <!-- http://localhost:5000/route/v1/driving/121.5335257,25.031712618;121.5338070318,25.0334311177 -->
  <node pkg="tf2_ros" type="static_transform_publisher" name="tf_map"
              args="328118 4691996 0 0 0 0 /world /loacal_map" />
  <node pkg="tf2_ros" type="static_transform_publisher" name="tf_map_earth"
              args="0 0 0 0 0 0 /earth /world" />
  <node pkg="pev_osrm" type="osrm_routing" name="osrm_route_node" output="screen">
    <param name="osrm_file" type="str" value="map" />
  </node>
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find pev_osrm)/rviz/osrm.rviz" />
  <include file="$(find osm_cartography)/launch/viz_osm.launch">
    <arg name="url" value="package://pev_odom/osm/media_lab_test.osm" />
  </include>
</launch>
