<!-- -*- mode: XML -*- -->

<launch>

  <arg name="file_name"/>

  <node pkg="rosbag" name="record" type="record" output="screen"
        args="-O $(env HOME)/bags/campusrover_bag/$(arg file_name) 
        /encoder_data
        /scan
        /velodyne_points
        /right_camera/color/camera_info
        /right_camera/color/image_raw
        /right_camera/aligned_depth_to_color/image_raw

        " />

</launch>
